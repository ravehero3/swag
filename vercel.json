{
  "buildCommand": "npm run build",
  "outputDirectory": "dist/public",
  "devCommand": "npm run dev",
  "framework": "vite",
  "env": {
    "NODE_ENV": {
      "required": false,
      "default": "production"
    },
    "DATABASE_URL": {
      "required": true
    },
    "SESSION_SECRET": {
      "required": true
    }
  }
}
